    "@storybook/html": "^6.4.22",
    "@types/jest": "^26.0.22",
    "babel-loader": "^8.2.5",
    "eslint": "^7.32.0",
    "jest": "^26.6.3",
    "scripts": "*",
    "tsconfig": "*",
    "tsup": "^5.10.1",
    "typescript": "^4.5.3"
  },
  "jest": {
    "preset": "scripts/jest/node"
  },
  "dependencies": {}
